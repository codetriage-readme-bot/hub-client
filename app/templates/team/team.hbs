<div class="row">
  <div class="col-md-2 l-left-container">
    {{layout/side-bar}}
  </div>
  <div class="col-md-10 l-main-container">
    {{layout/secondary-nav-bar}}
    <div class="l-cards-container">
      <div class="row l-title-container">
        <div class="col-md-10">
          <h2>
            <img src="/assets/fonts/user-list.svg" class="c-side-bar__icon" alt="Teams" />
            {{team.name}}
          </h2>
        </div>
        <div class="col-md-2 l-pull-right">
          {{teams/teams-settings team=model}}
        </div>
      </div>
      <div class="row">
        {{outlet}}
        <div class="col-md-6">
          <div class="card c-card">
            <div class="card-block c-card__block">
              <p class="c-card__description">
                {{{team.description}}}
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
